{
  "name": "crud-api",
  "version": "1.0.0",
  "description": "A simple CRUD API using Node.js and TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.ts",
    "start:dev": "ts-node-dev ./src/index.ts",
    "start:prod": "npm run build && node dist/index.js",
